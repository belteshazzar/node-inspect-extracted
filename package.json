{
  "name": "node-inspect-extracted",
  "version": "3.3.0",
  "description": "Node's util.inspect, extracted for use on the Web",
  "main": "./index.js",
  "type": "module",
  "exports": {
    "types": "./types/util.d.ts",
    "import": "./index.js"
  },
  "types": "./types/util.d.ts",
  "files": [
    "types/",
    "src/",
    "index.js"
  ],
  "scripts": {
    "check": "node tools/check",
    "lint": "eslint .",
    "build": "webpack",
    "pretest": "npm run build",
    "test": "node tools/runtests",
    "pretest:all": "npm run build",
    "test:all": "nve -p 14,16,18,20,22,24,25 node tools/runtests",
    "coverage": "c8 node tools/runtests",
    "preci": "npm run build && npm run lint",
    "ci": "c8 nve --no-progress -c -p 14,16,18,20,22,24,25 node tools/runtests"
  },
  "keywords": [
    "inspect",
    "util",
    "polyfill",
    "format",
    "output",
    "web",
    "browserify"
  ],
  "homepage": "https://github.com/hildjj/node-inspect-extracted",
  "repository": {
    "type": "git",
    "url": "http://github.com/hildjj/node-inspect-extracted.git"
  },
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "contributors": [
    "Noah May <noahmouse2011@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "7.28.5",
    "@babel/plugin-syntax-import-attributes": "7.27.1",
    "@babel/plugin-syntax-import-source": "7.27.1",
    "@babel/preset-env": "^7.28.5",
    "@eslint/js": "9.39.2",
    "@stylistic/eslint-plugin-js": "4.4.1",
    "babel-loader": "^10.0.0",
    "baseline-browser-mapping": "^2.9.7",
    "c8": "^10.1.3",
    "eslint": "^9.39.2",
    "eslint-plugin-jsdoc": "61.5.0",
    "eslint-plugin-markdown": "5.1.0",
    "globals": "16.5.0",
    "nve": "18.0.3",
    "semver": "7.7.3",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  },
  "packageManager": "pnpm@10.26.0",
  "engines": {
    "node": ">=14"
  },
  "pnpm": {
    "overrides": {
      "glob": "10.5.0",
      "js-yaml": "4.1.1"
    }
  }
}
