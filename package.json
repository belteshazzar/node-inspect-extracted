{
  "name": "node-inspect-extracted",
  "version": "3.3.0",
  "description": "Node's util.inspect, extracted for use on the Web",
  "main": "./dist/util.es.js",
  "type": "module",
  "exports": {
    "types": "./types/util.d.ts",
    "import": "./dist/util.es.js"
  },
  "types": "./types/util.d.ts",
  "files": [
    "types/",
    "dist/"
  ],
  "scripts": {
    "check": "node tools/check",
    "lint": "eslint .",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "http-server .",
    "test": "node tools/runtests",
    "test:all": "nve -p 14,16,18,20,22,24,25 node tools/runtests",
    "coverage": "c8 node tools/runtests",
    "preci": "npm run lint",
    "ci": "c8 nve --no-progress -c -p 14,16,18,20,22,24,25 node tools/runtests"
  },
  "keywords": [
    "inspect",
    "util",
    "polyfill",
    "format",
    "output",
    "web",
    "browserify"
  ],
  "homepage": "https://github.com/hildjj/node-inspect-extracted",
  "repository": {
    "type": "git",
    "url": "http://github.com/hildjj/node-inspect-extracted.git"
  },
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "contributors": [
    "Noah May <noahmouse2011@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@stylistic/eslint-plugin-js": "4.4.1",
    "c8": "^10.1.3",
    "eslint": "^9.39.2",
    "eslint-plugin-jsdoc": "61.5.0",
    "eslint-plugin-markdown": "5.1.0",
    "globals": "16.5.0",
    "http-server": "^14.1.1",
    "nve": "18.0.3",
    "semver": "7.7.3",
    "vite": "^6.4.1"
  },
  "engines": {
    "node": ">=14"
  }
}
